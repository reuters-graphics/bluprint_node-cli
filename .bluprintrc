{
  "bluprint": "^0.6.6",
  "name": "Node CLI",
  "category": "",
  "actions": [{
    "action": "prompt",
    "questions": [{
      "type": "text",
      "name": "projectSlug",
      "message": "What's the slug for your CLI, e.g., my-project?"
    }]
  }, {
    "action": "remove",
    "paths": ["README.md"]
  }, {
    "action": "move",
    "paths": ["PROJECT_README.md", "README.md"]
  }, {
    "action": "copy",
    "paths": [".env.example", ".env"]
  }, {
    "action": "render",
    "engine": "mustache",
    "files": ["LICENSE"]
  }, {
    "action": "regexreplace",
    "files": [
      "README.md",
      "HOME.md",
      "package.json",
      "tsconfig.json",
      "rollup.config.js",
      "test/index.ts",
      "src/index.ts",
      "src/cli.ts"
    ],
    "replace": [
      ["project-slug", "{{#slugify}}{{ projectSlug }}{{/slugify}}"],
      ["ProjectClass", "{{#camelize}}{{#capitalize}}{{#slugify}}{{ projectSlug }}{{/slugify}}{{/capitalize}}{{/camelize}}"],
      ["projectInstance", "{{#camelize}}{{#slugify}}{{ projectSlug }}{{/slugify}}{{/camelize}}"]
    ]
  }, {
    "action": "execute",
    "cmds": [
      ["pnpm", ["i"]],
      ["git", ["init"]],
      ["git", ["add", "."]],
      ["git", ["commit", "-m", "initial"]],
      ["npx", ["lefthook", "install"]]
    ]
  }, {
    "action": "log",
    "msg": "Ready to build {green {{ projectSlug }}}!"
  }]
}